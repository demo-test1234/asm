{
  "name": "server-MuseTalk",
  "cloudName": "CloudMuseTalk",
  "version": "0.3.0",
  "title": "MuseTalk模型",
  "description": "基于MuseTalk模型的模型，支持视频合成功能",
  "platformName": "__ENV__",
  "platformArch": "__ARCH__",
  "serverRequire": ">=0.3.0",
  "deviceDescription": "内存16G+，显存16GB+，支持CUDA加速",
  "entry": "launcher",
  "launcher": {
    "entry": "./_aienv/bin/python",
    "entryArgs": [
      "-u",
      "-m",
      "aigcpanelrun",
      "${CONFIG}"
    ],
    "envs": [
      "AAA=111",
      "BBB=222"
    ]
  },
  "functions": [
    "videoGen"
  ],
  "settings": [
    {
      "name": "port",
      "type": "text",
      "title": "服务端口",
      "default": "",
      "placeholder": "留空会检测使用随机端口"
    }
  ]
}
